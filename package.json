  "description": "Facebook Conceal for node and browser written in ts",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "browser": "lib/browser/facebook-conceal.bundle.js",
  "typings": "lib/cjs/index.d.ts",
  "files": [
    "lib/"
  ],
  "scripts": {
